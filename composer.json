{
    "name": "kenlog/docpht",
    "type": "project",
    "description": "With DocPHT you can take notes and quickly document anything and without the use of any database.",
    "homepage": "https://docpht.org",
    "keywords": [
        "Docpht",
        "Note-taking",
        "Documentation",
        "Markdown"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Valentino Pesce",
            "email": "valentino@iltuobrand.it",
            "role": "Developer"
        },
        {
            "name": "Craig Crosby",
            "email": "creecros@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "nezamy/route": "^1.2",
        "symfony/translation": "^4.3",
        "nette/forms": "^3.0",
        "erusev/parsedown":"^1.7",
        "nette/robot-loader": "^3.2",
        "plasticbrain/php-flash-messages": "^1.0",
        "nette/mail": "^3.0",
        "latte/latte": "^2.5",
        "geertw/ip-anonymizer": "^1.1",
        "cbschuld/browser.php": "^1.9",
        "ezimuel/php-secure-session": "^1.1",
        "php-curl-class/php-curl-class": "^8.6",
        "izadori/parsedown-plus": "^1.0",
        "google/apiclient": "^2.18"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"file_put_contents('vendor/nezamy/route/system/Route.php', file_get_contents('patches/Route.php'));\""
        ],
        "post-update-cmd": [
            "@php -r \"file_put_contents('vendor/nezamy/route/system/Route.php', file_get_contents('patches/Route.php'));\""
        ]
    },
    "config": {
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
