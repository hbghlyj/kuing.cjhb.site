    "homepage": "https://docpht.org",
    "keywords": [
        "Docpht",
        "Note-taking",
        "Documentation",
        "Markdown"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Valentino Pesce",
            "email": "valentino@iltuobrand.it",
            "role": "Developer"
